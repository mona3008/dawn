      ],
      "settings": {
        "custom_liquid": "<nav class=\"breadcrumb\" aria-label=\"Breadcrumb\">\n  <ol class=\"breadcrumb__list\">\n    <li class=\"breadcrumb__item\">\n      <a href=\"\/\" class=\"breadcrumb__link\">{{ 'general.breadcrumbs.home' | t }}<\/a>\n      {% if current_collection or product %}\n        <span class=\"breadcrumb__separator\">\/<\/span>\n      {% endif %}\n    <\/li>\n    {% if current_collection %}\n      <li class=\"breadcrumb__item\">\n        <a href=\"{{ current_collection.url }}\" class=\"breadcrumb__link\">{{ current_collection.title }}<\/a>\n        {% if product %}\n          <span class=\"breadcrumb__separator\">\/<\/span>\n        {% endif %}\n      <\/li>\n    {% endif %}\n    {% if product %}\n      <li class=\"breadcrumb__item\">\n        <span class=\"breadcrumb__link\" aria-current=\"page\">{{ product.title }}<\/span>\n      <\/li>\n    {% endif %}\n  <\/ol>\n<\/nav>",
